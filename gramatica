 <S>::= 'main' <exp1> | 'continue'| 'sizeof'
<exp1> ::= <operando> | <var> '=' <operando> <op> <operando> | <condicao> | <laco>
<exp2> ::= <var> <op2> <var>
<condicao> ::= 'if' <exp2> <S> 'break' 'else' <S> 'break'
<laco>::=   'while' <exp2> <exp1> 'return'
<operando> ::= <var> | <num> 
<var>::=    'a'<var>|'e'<var>|'i'<var>|'o'<var>|'u'<var> |'a'| 'e' |'i' | 'o' | 'u'
<num>::=    '1'<num>|'2'<num>|'3'<num>|'4'<num>|'5'<num>|'6'<num>|'7'<num>|'8'<num>|'9'<num>|'0'<num> | '1'|'2'|'3'|'4'|'5'|'6'|'7'|'8'|'9'|'0'
<op>::= '+'|'-'|'/'|'*'
<op2>::=    '==' | '<' | '>'
